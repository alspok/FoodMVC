@model IEnumerable<FoodMVC.Models.LogReg>

@{
    ViewBag.Title = "Vartotojai";
}

<h2>Vartotojai</h2>

<p>
    @Html.ActionLink("Naujas vartotojas", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Role)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastLog)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastMod)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Role)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastLog)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastMod)
        </td>
        <td>
            @Html.ActionLink("Keisti", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalės", "Details", new { id=item.Id }) |
            @Html.ActionLink("Trinti", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
